    "$type": "typography",
    "display": {
      "$value": {
        "fontFamily": "Antarctica, system-ui, sans-serif",
        "fontSize": "{font.size.display}",
        "fontWeight": "{font.weight.light}",
        "fontWidth": "{font.width.normal}",
        "lineHeight": "{font.lineHeight.tight}",
        "letterSpacing": "{font.letterSpacing.tightest}"
      }
    },
    "heading-lg": {
      "$value": {
        "fontFamily": "Antarctica, system-ui, sans-serif",
        "fontSize": "{font.size.lg}",
        "fontWeight": "{font.weight.normal}",
        "fontWidth": "{font.width.normal}",
        "lineHeight": "{font.lineHeight.tight}",
        "letterSpacing": "{font.letterSpacing.tight}"
      }
    },
    "body": {
      "$value": {
        "fontFamily": "Antarctica, system-ui, sans-serif",
        "fontSize": "{font.size.base}",
        "fontWeight": "{font.weight.normal}",
        "fontWidth": "{font.width.normal}",
        "lineHeight": "{font.lineHeight.normal}",
        "letterSpacing": "{font.letterSpacing.normal}"
      }
    },
    "body-sm": {
      "$value": {
        "fontFamily": "Antarctica, system-ui, sans-serif",
        "fontSize": "{font.size.sm}",
        "fontWeight": "{font.weight.normal}",
        "fontWidth": "{font.width.normal}",
        "lineHeight": "{font.lineHeight.normal}",
        "letterSpacing": "{font.letterSpacing.normal}"
      }
    },
    "label": {
      "$value": {
        "fontFamily": "Antarctica, system-ui, sans-serif",
        "fontSize": "{font.size.xs}",
        "fontWeight": "{font.weight.normal}",
        "fontWidth": "{font.width.normal}",
        "lineHeight": "{font.lineHeight.tight}",
        "letterSpacing": "{font.letterSpacing.wider}"
      }
    },
    "caption": {
      "$value": {
        "fontFamily": "Antarctica, system-ui, sans-serif",
        "fontSize": "{font.size.xs}",
        "fontWeight": "{font.weight.light}",
        "fontWidth": "{font.width.normal}",
        "lineHeight": "{font.lineHeight.normal}",
        "letterSpacing": "{font.letterSpacing.normal}"
      }
    }
  },
  "component": {
    "button": {
      "$value": {
        "paddingX": "{spacing.4}",
        "paddingY": "{spacing.2}",
        "borderRadius": "{radius.sm}",
        "typography": "{typography.body-sm}"
      },
      "$description": "button"
    },
    "button-pill": {
      "$value": {
        "paddingX": "{spacing.4}",
        "paddingY": "{spacing.2}",
        "borderRadius": "{radius.full}",
        "typography": "{typography.body-sm}"
      },
      "$description": "button-pill"
    },
    "card": {
      "$value": {
        "padding": "{spacing.4}",
        "borderRadius": "{radius.md}",
        "shadow": "inset",
        "border": "default"
      },
      "$description": "card"
    },
    "input": {
      "$value": {
        "paddingX": "{spacing.3}",
        "paddingY": "{spacing.3}",
        "borderRadius": "{radius.md}",
        "typography": "{typography.body}"
      },
      "$description": "input"
    },
    "tag": {
      "$value": {
        "paddingX": "{spacing.2-5}",
        "paddingY": "{spacing.1-5}",
        "borderRadius": "{radius.full}",
        "typography": "{typography.label}"
      },
      "$description": "tag"
    },
    "modal": {
      "$value": {
        "padding": "{spacing.6}",
        "borderRadius": "{radius.md}",
        "shadow": "sm",
        "border": "none"
      },
      "$description": "modal"
    },
    "avatar": {
      "$value": {
        "size": "{spacing.8}",
        "borderRadius": "{radius.full}",
        "border": "subtle"
      },
      "$description": "avatar"
    },
    "checkbox": {
      "$value": {
        "size": "{spacing.5}",
        "borderRadius": "{radius.sm}",
        "border": "default"
      },
      "$description": "checkbox"
    },
    "toggle": {
      "$value": {
        "width": "{spacing.12}",
        "height": "{spacing.5}",
        "borderRadius": "{radius.full}"
      },
      "$description": "toggle"
    }
  },
  "shadow": {
    "sm": {
      "$value": "rgba(0, 0, 0, 0.1) 0px 1px 3px 0px, rgba(0, 0, 0, 0.1) 0px 1px 2px -1px"
    },
    "inset": {
      "$value": "rgba(255, 255, 255, 0.5) 0px 0px 4px 1px inset"
    }
  }
}